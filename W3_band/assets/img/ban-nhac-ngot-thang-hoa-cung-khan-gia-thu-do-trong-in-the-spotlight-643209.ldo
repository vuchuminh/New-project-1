    window.onload = function () {
        gapi.load('auth2', function () {

            //function signOut() {
            //    var auth = gapi.auth2.getAuthInstance();
            //    auth.signOut().then(function () {
            //        console.log('User signed out.');
            //    });
            //}

            var auth2 = gapi.auth2.init({
                client_id: '592710429347-7q9qlqu638ht2kanr8lbhlr622o7rdrq.apps.googleusercontent.com',
                cookiepolicy: 'single_host_origin',
                scope: 'profile email'
            });

            document.getElementById('google-sign-in').addEventListener('click', function () {
                auth2.signIn().then(() => {
                    var profile = auth2.currentUser.get().getBasicProfile();

                    var token = auth2.currentUser.get().getAuthResponse().access_token;
                    //gapi.auth2.getAuthInstance().currentUser.get().getAuthResponse().access_token;
                    //console.log(profile);

                    console.log(token);

                    console.log('ID: ' + profile.getId()); // Do not send to your backend! Use an ID token instead.
                    console.log('Name: ' + profile.getName());
                    console.log('Image URL: ' + profile.getImageUrl());
                    console.log('Email: ' + profile.getEmail()); // This is null if the 'email' scope is not present.
                    //signOut();

                }).catch((error) => {
                    console.error('Google Login Error: ', error)
